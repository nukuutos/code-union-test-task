{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2022
  },
  "extends": ["airbnb", "airbnb/hooks", "plugin:react/jsx-runtime", "prettier"],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/function-component-definition": [2, { "namedComponents": "arrow-function" }]
  }
}
